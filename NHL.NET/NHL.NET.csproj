<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>netstandard2.0</TargetFramework>
		<PackageId>NHL.NET</PackageId>
		<VersionPrefix>1.0.0</VersionPrefix>
		<Authors>NolanBradshaw</Authors>
		<Description>C# API Wrapper for the undocumented NHL API</Description>
		<PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
		<PackageProjectUrl>https://github.com/nolanbradshaw/NHL.NET</PackageProjectUrl>
		<PackageLicenseUrl>https://github.com/nolanbradshaw/NHL.NET/blob/main/LICENSE.md</PackageLicenseUrl>
		<Copyright>Copyright (c) Nolan Bradshaw 2021-2022</Copyright>
		<PackageTags>nhl-api nhl national-hockey-league</PackageTags>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
	</ItemGroup>

</Project>
